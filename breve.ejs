<script type="text/javascript">
	var counter = {
		init: function (d)
		{
			jQuery('#Counter').countdown({
				until: new Date(d),
				layout: counter.layout(),
				labels: ['anos', 'meses', 'semanas', 'dias', 'horas', 'min', 'seg'],
				labels1: ['ano', 'mês', 'semana', 'dia', 'hora', 'min', 'seg']
			});
		},
		layout: function ()
		{
			return '<li>{dn}<span>{dl}</span></li>' + 
						'<li>{hnn}<span>{hl}</span></li>' + 
						'<li>{mnn}<span>{ml}</span></li>' + 
						'<li>{snn}<span>{sl}</span></li>' + 
						'<li class="till_lauch"><img src="images/rocket.png"></li>';
		}
	}
		
	// initialize the counter
	counter.init("March 01, 2013");

</script>
